{
  "manifest_version": 3,
  "name": "BrainBox - AI Chat Organizer",
  "version": "2.2.0",
  "description": "Organize AI conversations from ChatGPT, Gemini, Claude, Grok, Perplexity, DeepSeek, and Qwen into your personal dashboard",
  "permissions": [
    "storage",
    "webRequest",
    "cookies",
    "contextMenus",
    "notifications",
    "tabs",
    "scripting",
    "activeTab",
    "alarms"
  ],
  "host_permissions": [
    "http://localhost/*",
    "http://127.0.0.1/*",
    "https://brainbox-alpha.vercel.app/*",
    "https://chatgpt.com/*",
    "https://chat.openai.com/*",
    "https://claude.ai/*",
    "https://gemini.google.com/*",
    "https://chat.deepseek.com/*",
    "https://api.deepseek.com/*",
    "https://www.perplexity.ai/*",
    "https://chat.qwenlm.ai/*",
    "https://chat.lmsys.org/*",
    "https://grok.com/*",
    "https://*.x.ai/*",
    "https://*.x.com/*",
    "https://*.twitter.com/*",
    "https://*.perplexity.ai/*",
    "https://dashscope.aliyuncs.com/*",
    "https://api.together.xyz/*",
    "https://lmarena.ai/*",
    "https://arena.ai/*",
    "https://chat.qwen.ai/*",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "src/background/service-worker.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/content-chatgpt.js",
        "src/prompt-inject/prompt-inject.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://claude.ai/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/content-claude.js",
        "src/prompt-inject/prompt-inject.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://gemini.google.com/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/brainbox_master.js",
        "src/prompt-inject/prompt-inject.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/prompt-inject/prompt-inject.js"
      ],
      "run_at": "document_idle",
      "exclude_matches": [
        "https://gemini.google.com/*"
      ]
    },
    {
      "matches": [
        "https://chat.deepseek.com/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/content-deepseek.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.perplexity.ai/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/content-perplexity.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://x.com/i/grok*",
        "https://grok.com/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/content-grok.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://chat.qwen.ai/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/content-qwen.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://chat.lmsys.org/*",
        "https://arena.ai/*"
      ],
      "js": [
        "src/lib/ui.js",
        "src/content/content-lmarena.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://brainbox-alpha.vercel.app/extension-auth",
        "http://localhost:3000/extension-auth",
        "http://127.0.0.1:3000/extension-auth"
      ],
      "js": [
        "src/content/content-dashboard-auth.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "src/icons/icon16.png",
      "32": "src/icons/icon32.png",
      "48": "src/icons/icon48.png",
      "128": "src/icons/icon128.png"
    }
  },
  "icons": {
    "16": "src/icons/icon16.png",
    "32": "src/icons/icon32.png",
    "48": "src/icons/icon48.png",
    "128": "src/icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "src/icons/*",
        "src/content/inject-gemini-main.js"
      ],
      "matches": [
        "https://gemini.google.com/*"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'; frame-ancestors 'none';"
  }
}